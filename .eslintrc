{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": [
    "flow-vars"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1,
    "comma-dangle": [2, "always-multiline"],
    "no-unused-vars": [2, {
      "args": "none",
      "vars": "all"
    }],
    "no-console": 0
  }
}